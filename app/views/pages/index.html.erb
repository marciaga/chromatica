<!-- graph gets rendered below -->
<div class="row">
	<div class="col-lg-12">
		<div class="row">
			<% if !current_user %>
				<%= render 'partials/splash' %>
			<% end %>
		  <div class="col-md-9 centered">
		  	<div id="graph">
		  	</div>
		  </div>
				<% if current_user %>
					<div id="searches" class="col-md-3 centered history" >
						<% auth_token_check %>
						<%= render "partials/sidebar" %>
					</div>
				<% end %>
		</div>
	</div>
</div>
<!-- initializes the search function -->
<script type="text/javascript">
  $(function() {
    relatedArtistSearch.init();
  });
</script>

	